  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@workos-inc/authkit-react": "^0.11.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "convex": "workspace:*",
    "vitest": "^3.2.4",
    "zshy": "0.4.1"
  },
  "peerDependencies": {
    "convex": "^1.25.0",
    "react": "^18.0.0 || ^19.0.0-0 || ^19.0.0"
  },
  "zshy": "./src/index.tsx",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
