    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.11",
